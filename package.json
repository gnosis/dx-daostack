{
  "name": "@gnosis.pm/dx-daostack",
  "version": "0.1.3",
  "description": "",
  "main": "index.js",
  "config": {
    "gasLimit": "7000000"
  },
  "scripts": {
    "test": "truffle test test/*.js",
    "test-debug": "DEBUG=app:*,test:* truffle test test/*.js",
    "rpc": "ganache-cli -d",
    "networks-extract": "npx truffle networks --clean && npx tnt extractNetworks",
    "networks-inject": "npx tnt injectNetworks",
    "networks-reset": "truffle networks --clean && npm run networks-inject",
    "compile": "truffle compile",
    "migrate": "truffle migrate",
    "preversion": "npm run restore",
    "restore": "rm -rf build && npm run compile && npm run networks-reset",
    "networks": "truffle networks",
    "lint": "eslint .",
    "solium": "solium --dir ./contracts/",
    "get-0x-list": "truffle exec src/scripts/get-0x-list.js",
    "get-token-prices": "node src/scripts/get-token-prices.js",
    "update-price-oracle": "node src/scripts/update-price-oracle.js",
    "get-cryptocompare-tokens": "node src/scripts/get-cryptocompare-tokens"
  },
  "license": "ISC",
  "devDependencies": {
    "@gnosis.pm/truffle-nice-tools": "^1.2.1",
    "babel-eslint": "^10.0.1",
    "dotenv": "^6.1.0",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "got": "^9.3.2",
    "moment-timezone": "^0.5.23",
    "solium": "^1.1.8",
    "truffle": "^5.0.0-user-level-mnemonic.0",
    "yargs": "^12.0.5"
  },
  "dependencies": {
    "@daostack/arc": "0.0.0-alpha.56",
    "@daostack/arc.js": "0.0.0-alpha.89",
    "@gnosis.pm/dx-contracts": "^2.0.0-SNAPSHOT.5",
    "@gnosis.pm/gno-token": "^1.1.2",
    "@gnosis.pm/owl-token": "^1.2.2",
    "@gnosis.pm/util-contracts": "1.1.1",
    "debug": "^4.1.0",
    "truffle-contract": "^3.0.6",
    "truffle-hdwallet-provider": "^1.0.0-web3one.1"
  }
